#include <iostream>
using namespace std;

bool isDigitsAscending(int n){
    while (n > 10){
        int x = n % 10;
        n /= 10;
        int y = n % 10;
        if (y < x){
            return false;
        }
    }
    return true;
}

int main()
{
    int n;
    cout<<"Input n: ";
    cin>>n;
    if (isDigitsAscending){
        cout<<"It is ascending: "<<n;
    }
    return 0;
}
